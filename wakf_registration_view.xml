	                           				<field name="month"/>
	                           				<field name="days"/>
				
                        				</group>
                        				<group>
	                        				<field name="ben_wakf"/>
	                           				<field name="check_required"/>
                        				</group>
                        			</form>
                        		</field>
                        	</page>
