			                        <field name="panchayath" attrs="{'readonly':[('state1','in',['approved','sanctioned','rejected','re_payment','paid','revolving','finished'])]}"/>
			                        <field name="taluk_id" attrs="{'readonly':[('state1','in',['approved','sanctioned','rejected','re_payment','paid','revolving','finished'])]}"/>
