					                        <field name="year"/>
					                        <field name="date_from"/>
					                        <field name="date_to"/>
					                        <field name="amount" sum="Total Amount"/>
					                        <field name="state"/>

                        			</tree>
                        		
                        		</field>
                        	</page>
                        	
                    </xpath> 
