                        			<tree string="Rejected Reason">
                        					<field name="name"/>	
                        			</tree>
                        			<form string="Rejected Reason">
                        			    <group>
					                        <field name="over_income"/>
					                        <field name="grant_sanctioned"/>
					                        <field name="after_marriage"/>
					                        <field name="age_not_specified"/>
					                        <field name="under_age"/>
					                        <field name="exp_not_spcfed"/>
					                        <field name="under_experience"/>
					                        <field name="job_income_not"/>
					                        <field name="no_reason"/>
					                        <field name="no_annual_income"/>
					                        <field name="mahal_not_regd"/>
					                        <field name="undefined_ctgry"/>
					                        <field name="certificate_not"/>
					                        <field name="institue_not"/>
                        				</group>
                        			</form>
                        		
                        		</field>
                        	</page>
                        	<page string="Previous Transactions" attrs="{'invisible':['|',('is_wakf', '=', True),('categ_name','!=','Pension')]}">
                        		<field name="history_transaction" nolabel="1" edit="False" delete="False">
                        			<tree string="Transactions">	    
					                        <field name="year"/>
					                        <field name="date_sanction"/>
					                        <field name="for_month"/>
					                        <field name="amount" sum="Total Amount"/>
					                        <field name="dd_no"/>
					                        <field name="transaction_no"/>
					                        <field name="status"/>
                        			</tree>
                        		
                        		</field>
                        	</page>
                        	<page string="Loan By Year" attrs="{'invisible':['|',('is_wakf', '=', True),('categ_name','!=','Education Loan')]}">
