                    	<button name="action_submit" string="Submit" type="object" groups="wakf_registration.group_sws_submit" icon="gtk-go-forward" attrs="{'invisible':['|',('state1','in',('submitted','verified','approved','sanctioned','revolving','finished','rejected','re_payment','paid')),('is_person', '!=', True)]}"/>
                    	<button name="action_verify1" string="Verify" type="object" groups="wakf_registration.group_sws_submit" icon="gtk-go-forward" attrs="{'invisible':['|',('state1','!=','submitted'),('is_person', '!=', True)]}"/>
                		<button name="action_approve1" string="Approve" type="object" groups="wakf_registration.group_sws_submit" icon="gtk-go-forward" attrs="{'invisible':['|',('is_person', '!=', True),('state1', '!=', 'verified')]}"/>
