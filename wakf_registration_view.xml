			                        <field name="category" attrs="{'required':[('state1','in',['submitted','verified','cancelled','approved','sanctioned'])],'readonly':[('state1','in',['approved','sanctioned','rejected','re_payment','paid','revolving','finished'])]}" domain="[('categ_id','=',head)]" options="{'limit': 25, 'create': false, 'create_edit': false}"/>
			                        <field name="appli_date" attrs="{'required':[('state1','in',['submitted','verified','cancelled','approved','sanctioned'])],'readonly':[('state1','in',['approved','sanctioned','rejected','re_payment','paid','revolving','finished'])]}"/>
			                        <field name="full_name" attrs="{'required':[('state1','in',['submitted','verified','cancelled','approved','sanctioned'])],'readonly':[('state1','in',['approved','sanctioned','rejected','re_payment','paid','revolving','finished'])]}"/>
